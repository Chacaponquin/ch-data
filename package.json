{
  "name": "ch-data",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run test && npm run prettier-format && npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prepack": "npm run build",
    "test": "mocha --r ts-node/register test/**/*.ts",
    "test:lib": "mocha --r ts-node/register test/lib/**/*.ts",
    "test:schemas": "mocha --r ts-node/register test/schemas/**/*.ts",
    "prettier-format": "prettier --config .prettierrc src/**/*.ts --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Chacaponquin/ch-data.git"
  },
  "keywords": [
    "ch-data",
    "fake-data",
    "generate data",
    "random data"
  ],
  "author": "chacaponquin <hectorangel2001@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.8.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "chai": "^4.3.6",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "json2csv": "^5.0.7",
    "uuid": "^9.0.0"
  }
}
